{
  "name": "edhrec_average_deck",
  "description": "Fetch EDHREC Average Deck for a commander (all/exhibition[/budget|/expensive]/core/upgraded/optimized/cedh[/budget|/expensive]).",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "description": "Commander name"
      },
      "bracket": {
        "type": "string",
        "description": "Bracket (name or 1..5)",
        "enum": [
          "all",
          "exhibition",
          "exhibition/budget",
          "exhibition/expensive",
          "core",
          "upgraded",
          "optimized",
          "cedh",
          "cedh/budget",
          "cedh/expensive",
          "1",
          "2",
          "3",
          "4",
          "5"
        ],
        "default": "all"
      }
    },
    "required": ["name"]
  },
  "route": "/edhrec/average-deck"
}
